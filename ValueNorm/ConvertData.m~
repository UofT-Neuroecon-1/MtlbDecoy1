load('simulout.mat')

for s=1:30
    for t=1:270
        SubjData{s}.Xs{t}=V((s-1)*270+t,:)';
    end
    SubjData{s}.ChoiceList=choice{1}((s-1)*270+t);
end
save ExampleData3 'SubjData' 'par'